{
  "name": "react-native-bubbles",
  "version": "0.3.1",
  "description": "Bubbles for React Native",
  "private": false,
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "files": [
    "src",
    "lib",
    "babel.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/packagex-io/bubbles-react-native.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "prestorybook": "rnstl",
    "dev-android": "concurrently \"npm run storybook\" \"npm run start\" \"npm run android\"",
    "dev-ios": "concurrently \"npm run storybook\" \"npm run start\" \"npm run ios\"",
    "release": "release-it",
    "prepare": "bob build "
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.7",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "color": "^4.2.3",
    "react-native-mask-input": "^1.2.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-vector-icons": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/color": "^3.0.3",
    "@types/jest": "^26.0.23",
    "@types/react-dom": "^18.0.5",
    "@types/react-native": "^0.67.3",
    "@types/react-native-vector-icons": "^6.4.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-native-web": "^0.18.9",
    "concurrently": "^7.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-storybook": "^0.6.4",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-dom": "^18.2.0",
    "react-native-builder-bob": "^0.20.0",
    "react-native-config": "^1.4.5",
    "react-native-storybook-loader": "^2.0.5",
    "react-test-renderer": "17.0.2",
    "release-it": "^15.5.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./storybook/stories"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ],
    "files": [
      "src/"
    ]
  }
}
